<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="tradelr.com" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="tradelr">
    <InputEndpoints>
      <InputEndpoint name="HttpIn" protocol="http" port="80" />
      <InputEndpoint name="HttpsIn" protocol="https" port="443" />
    </InputEndpoints>
    <LocalStorage name="search" sizeInMB="1500" />
    <ConfigurationSettings>
      <Setting name="DataConnectionString" />
      <Setting name="DiagnosticsConnectionString" />
    </ConfigurationSettings>
    <LocalResources>
    </LocalResources>
    <Certificates>
    </Certificates>
  </WebRole>
</ServiceDefinition>